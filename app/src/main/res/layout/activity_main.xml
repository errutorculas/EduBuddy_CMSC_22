<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <View
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/White"
            android:id="@+id/view"/>
    <ImageButton
            android:id="@+id/menu_button"
            android:layout_width="29dp"
            android:layout_height="19dp"
            android:background="@drawable/menu"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.139"
            app:layout_constraintVertical_bias="0.07"
            tools:ignore="ContentDescription"/>
    <ImageButton
            android:layout_width="43dp"
            android:layout_height="43dp"
            android:background="@drawable/profile1"
            tools:layout_editor_absoluteY="36dp"
            tools:layout_editor_absoluteX="322dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.87"
            app:layout_constraintVertical_bias="0.06"
            tools:ignore="ContentDescription"/>
    <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_baseline_notifications_none_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.74"
            app:layout_constraintVertical_bias="0.07"
            tools:ignore="ContentDescription"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/hi_juan"
            android:textSize="42sp"
            android:textColor="#15096F"
            android:fontFamily="@font/productsansbold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.203"
            app:layout_constraintVertical_bias="0.239"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/grade"
            android:textSize="14sp"
            android:textColor="#15096F"
            android:fontFamily="@font/ssplight"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.163"
            app:layout_constraintVertical_bias="0.34"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/inhs"
            android:textSize="18sp"
            android:textColor="#15096F"
            android:fontFamily="@font/sspsemibold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.139"
            app:layout_constraintVertical_bias="0.303"/>
    <ImageButton
            android:id="@+id/search_button"
            android:layout_width="320dp"
            android:layout_height="80dp"
            android:background="@drawable/search"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintVertical_bias="0.43"
            tools:ignore="ContentDescription"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/search"
            android:textSize="14sp"
            android:textColor="#15096F"
            android:fontFamily="@font/ssplight"
            tools:layout_editor_absoluteY="270dp"
            tools:layout_editor_absoluteX="90dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintVertical_bias="0.43"
            tools:ignore="ContentDescription"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/subjects"
            android:textSize="18sp"
            android:textColor="#15096F"
            android:fontFamily="@font/sspsemibold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.15"
            app:layout_constraintVertical_bias="0.543"/>

    <LinearLayout
            android:layout_width="380dp"
            android:layout_height="161dp"
            app:layout_constraintBottom_toBottomOf="@+id/view"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="-0.10"
            app:layout_constraintVertical_bias="0.750">
        <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scrollbars="none"
                tools:ignore="ExtraText,UselessParent">
            <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                <ImageButton
                        android:id="@+id/english_button"
                        android:layout_width="140dp"
                        android:layout_height="160dp"
                        android:background="@drawable/englishbutton"
                        tools:ignore="ContentDescription"/>
                <ImageButton
                        android:id="@+id/math_button"
                        android:layout_width="140dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/mathbutton"
                        tools:ignore="ContentDescription"/>
                <ImageButton
                        android:id="@+id/science_button"
                        android:layout_width="140dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/sciencebutton"
                        tools:ignore="ContentDescription"/>
                <ImageButton
                        android:id="@+id/history_button"
                        android:layout_width="140dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/historybutton"
                        tools:ignore="ContentDescription"/>
            </LinearLayout>
        </HorizontalScrollView>
    </LinearLayout>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/modules"
            android:textSize="16sp"
            android:textColor="#15096F"
            android:fontFamily="@font/sspsemibold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.147"
            app:layout_constraintVertical_bias="0.838"/>
    <ImageButton
            android:layout_width="93dp"
            android:layout_height="93dp"
            android:background="@drawable/forumbutton"
            tools:layout_editor_absoluteY="554dp"
            tools:layout_editor_absoluteX="62dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.147"
            app:layout_constraintVertical_bias="0.980"
            tools:ignore="ContentDescription"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/quizzes"
            android:textSize="16sp"
            android:textColor="#15096F"
            android:fontFamily="@font/sspsemibold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.553"
            app:layout_constraintVertical_bias="0.838"/>
    <ImageButton
            android:layout_width="93dp"
            android:layout_height="93dp"
            android:background="@drawable/quizzesbutton"
            tools:layout_editor_absoluteY="554dp"
            tools:layout_editor_absoluteX="190dp"
            tools:ignore="ContentDescription"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.553"
            app:layout_constraintVertical_bias="0.980"/>
</androidx.constraintlayout.widget.ConstraintLayout>